  "dependencies": {
    "@neural-trader/predictor": "workspace:*",
    "@neural-trader/core": "workspace:*",
    "agentdb": "^1.0.0",
    "agentic-flow": "^1.0.0",
    "openai": "^4.0.0",
    "numeric": "^1.2.6",
    "mathjs": "^12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/numeric": "^1.2.2",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
