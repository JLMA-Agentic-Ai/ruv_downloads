{
  "name": "@neural-trader/example-portfolio-optimization",
  "version": "1.0.0",
  "description": "Self-learning portfolio optimization with benchmark swarms and multi-objective optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "benchmark": "node dist/benchmark-swarm.js",
    "example:basic": "node dist/examples/basic-optimization.js",
