{
  "name": "@neural-trader/example-healthcare-optimization",
  "version": "1.0.0",
  "description": "Healthcare optimization with self-learning patient forecasting and swarm-based staff scheduling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "dev": "tsx watch src/index.ts",
    "example": "tsx examples/basic-usage.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "healthcare",
    "optimization",
    "machine-learning",
    "queue-theory",
    "scheduling",
    "forecasting",
    "neural-trader",
    "patient-flow",
    "hospital-management"
  ],
  "author": "Neural Trader",
  "license": "MIT",
  "dependencies": {
    "@neural-trader/predictor": "workspace:*",
    "agentdb": "^1.0.0",
    "agentic-flow": "^2.0.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [".ts"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  }
}
