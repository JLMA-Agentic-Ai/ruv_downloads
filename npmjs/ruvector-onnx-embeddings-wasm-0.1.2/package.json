    "ruvector_onnx_embeddings_wasm.js",
    "ruvector_onnx_embeddings_wasm.d.ts",
    "loader.js",
    "parallel-embedder.mjs",
    "parallel-worker.mjs",
    "LICENSE",
    "README.md"
