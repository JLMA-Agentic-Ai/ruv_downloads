{
  "host": "0.0.0.0",
  "port": 3000,
  "protocol": "http",
  "cors": {
    "origins": [
      "http://localhost:3000",
      "http://localhost:3001",
      "http://127.0.0.1:*"
    ],
    "credentials": true,
    "methods": ["GET", "POST", "OPTIONS"],
    "allowed_headers": ["Content-Type", "Authorization"]
  },
  "security": {
    "require_auth": false,
    "rate_limit_requests_per_minute": 600,
    "rate_limit_per_user": true,
    "helmet_enabled": true,
    "csp_enabled": false
  },
  "logging": {
    "level": "info",
    "format": "json",
    "audit_enabled": true
  }
}
